{
  "name": "bet-gaming",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cross-env": "^5.2.0",
    "dotenv-flow": "^0.1.0"
  },
  "scripts": {
    "start": "npm-run-all -p start-front start-back",
    "start-front": "react-scripts start",
    "start-back": "cross-env NODE_ENV=development nodemon server.js",
    "start-prod": "cross-env NODE_ENV=production node server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "history": "^4.7.2",
    "husky": "^1.0.0-rc.14",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.5",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5"
  }
}
